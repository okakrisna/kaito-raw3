<script>
jQuery(document).ready(function ($) {
  // =============== KONFIG ===============
  const ABSENT_PATTERNS = [
    /tidak\s*hadir/i, /tdk\s*hadir/i, /g(a|k)\s*hadir/i, /ngg?k\s*hadir/i,
    /unable/i, /tidak\s*bisa\s*(hadir|datang)/i, /tidak\s*datang/i, /\bno\b/i, /decline/i
  ];
  const AJAX_URL =
    (window.elementorFrontendConfig && elementorFrontendConfig.ajaxurl) ||
    (window.ajaxurl) || '/wp-admin/admin-ajax.php';

  // Field yang TIDAK ingin diulang di bagian "field tambahan"
  const EXCLUDE_IDS = new Set([
    'form-field-name',
    'form-field-attendance',
    'form-field-whatsapp',
    'form-field-whatsapp2',  // Nomor kedua
    'form-field-field_b0d08a4',
    'form-field-comment',
    'form-field-post_title',
    'form-field-link_undangan'
  ]);

  // =============== HELPER DASAR ===============
  function ensureHtml2Canvas() {
    if (window.html2canvas) return Promise.resolve(window.html2canvas);
    return new Promise((resolve, reject) => {
      const existing = document.querySelector('script[src*="html2canvas"]');
      const s = existing || document.createElement('script');
      s.src = 'https://cdnjs.cloudflare.com/ajax/libs/html2canvas/1.4.1/html2canvas.min.js';
      s.onload = () => resolve(window.html2canvas);
      s.onerror = reject;
      if (!existing) document.head.appendChild(s);
    });
  }

  function loadImages(node) {
    const imgs = Array.from(node.querySelectorAll('img'))
      .filter(img => !img.complete || img.naturalWidth === 0);
    return imgs.length ? Promise.all(imgs.map(img => new Promise(res => {
      if (!img.crossOrigin) img.crossOrigin = 'anonymous';
      img.addEventListener('load', res, { once: true });
      img.addEventListener('error', res, { once: true });
    }))) : Promise.resolve();
  }

  async function captureAreaAsJPEG(quality = 0.85) {
    await ensureHtml2Canvas();
    const target =
      document.querySelector('[data-qr-capture]') ||
      document.querySelector('#qr-download') ||
      document.querySelector('.qr-download');
    if (!target) return null;

    if (document.fonts && document.fonts.ready) { try { await document.fonts.ready; } catch(_) {} }

    const styleTag = document.createElement('style');
    styleTag.textContent = `
      .__capture_fix, .__capture_fix * {
        transition: none !important;
        animation: none !important;
        filter: none !important;
        backdrop-filter: none !important;
      }`;
    document.head.appendChild(styleTag);

    const prevStyle = target.getAttribute('style');
    target.classList.add('__capture_fix');

    const cs = getComputedStyle(target);
    const wasHidden = cs.display === 'none' || cs.visibility === 'hidden' || cs.opacity === '0';
    if (wasHidden) {
      target.style.setProperty('display','block','important');
      target.style.setProperty('position','fixed','important');
      target.style.setProperty('left','-10000px','important');
      target.style.setProperty('top','-10000px','important');
      target.style.setProperty('opacity','1','important');
      target.style.setProperty('visibility','visible','important');
      target.style.setProperty('transform','none','important');
    }

    await new Promise(r => requestAnimationFrame(r));
    const rect = target.getBoundingClientRect();
    if (rect.width && rect.height) {
      target.style.setProperty('width', rect.width + 'px', 'important');
      target.style.setProperty('height', rect.height + 'px', 'important');
    }
    await loadImages(target);

    try {
      const scale = Math.max(2, Math.ceil(window.devicePixelRatio || 1));
      const canvas = await html2canvas(target, {
        useCORS: true, allowTaint: false, backgroundColor: '#FFFFFF', scale, logging: false
      });
      return canvas.toDataURL('image/jpeg', quality);
    } finally {
      if (prevStyle === null) target.removeAttribute('style');
      else target.setAttribute('style', prevStyle);
      target.classList.remove('__capture_fix');
      styleTag.remove();
    }
  }

  function getEventTitle() {
    const wrap = document.querySelector('[data-qr-capture]');
    const hs = wrap ? wrap.querySelectorAll('.elementor-heading-title') : [];
    const t1 = (hs[0]?.textContent || '').trim() || 'The Wedding of';
    const t2 = (hs[1]?.textContent || '').trim() || '';
    return [t1, t2].filter(Boolean).join(' ');
  }

  function getRadioLabelText(inputEl, form) {
    if (!inputEl) return '';
    const byFor = form.querySelector(`label[for="${inputEl.id}"]`);
    if (byFor && byFor.textContent.trim()) return byFor.textContent.trim();
    const opt = inputEl.closest('.elementor-field-option');
    const lbl = opt ? opt.querySelector('label') : null;
    return (lbl && lbl.textContent.trim()) ? lbl.textContent.trim() : (inputEl.value || '').trim();
  }

  function isAbsent(attendanceInput, statusText) {
    if (!attendanceInput) return false;
    const ds = (attendanceInput.getAttribute('data-status') || '').toLowerCase();
    if (ds === 'absent') return true;
    if (ds === 'attend') return false;
    const val = (attendanceInput.value || '').trim().toLowerCase();
    const hay = (statusText + ' ' + val).toLowerCase();
    return ABSENT_PATTERNS.some(rx => rx.test(hay));
  }

  // =============== FIELD DINAMIS: kumpulkan semua field lain ===============
  function getElementorFieldLabel(inputEl) {
    const group = inputEl.closest('.elementor-field-group');
    const glabel = group ? group.querySelector('.elementor-field-label') : null;
    if (glabel && glabel.textContent.trim()) return glabel.textContent.trim();

    const form = inputEl.form || document;
    const byFor = inputEl.id ? form.querySelector(`label[for="${inputEl.id}"]`) : null;
    if (byFor && byFor.textContent.trim()) return byFor.textContent.trim();

    if (inputEl.placeholder && inputEl.placeholder.trim()) return inputEl.placeholder.trim();

    const nm = inputEl.name || '';
    const m = nm.match(/form_fields\[(.+?)\]/);
    if (m) return m[1].replace(/[_-]+/g, ' ').replace(/\b\w/g, s => s.toUpperCase());

    return inputEl.id || 'Field';
  }

  function collectExtraFields(form) {
    const items = [];
    const seenGroup = new Set();
    const all = form.querySelectorAll('input, select, textarea');

    all.forEach(el => {
      if (el.disabled) return;

      const id = el.id || '';
      if (EXCLUDE_IDS.has(id)) return;

      const type = (el.type || '').toLowerCase();
      if (['submit', 'button', 'reset', 'file'].includes(type)) return;

      if (el.name && !/form_fields\[/.test(el.name)) return;

      if (type === 'radio') {
        const key = el.name || el.id;
        if (seenGroup.has(key)) return;
        seenGroup.add(key);

        const checked = form.querySelector(`input[name="${CSS.escape(el.name)}"]:checked`);
        if (!checked) return;
        const label = getElementorFieldLabel(checked);
        const value = getRadioLabelText(checked, form);
        if (value) items.push({ label, value });
        return;
      }

      if (type === 'checkbox') {
        const key = el.name || el.id;
        if (seenGroup.has(key)) return;
        seenGroup.add(key);

        const checks = Array.from(form.querySelectorAll(`input[name="${CSS.escape(el.name)}"]`)).filter(c => c.checked);
        if (!checks.length) return;
        const label = getElementorFieldLabel(el);
        const value = checks.map(c => getRadioLabelText(c, form) || c.value).filter(Boolean).join(', ');
        if (value) items.push({ label, value });
        return;
      }

      if (el.tagName === 'SELECT') {
        const sel = el;
        const label = getElementorFieldLabel(sel);
        let value = '';
        if (sel.multiple) {
          value = Array.from(sel.selectedOptions).map(o => (o.textContent || o.label || o.value).trim()).filter(Boolean).join(', ');
        } else {
          const opt = sel.selectedOptions && sel.selectedOptions[0] ? sel.selectedOptions[0] : null;
          value = (opt ? (opt.textContent || opt.label || opt.value) : sel.value || '').trim();
        }
        if (value) items.push({ label, value });
        return;
      }

      const label = getElementorFieldLabel(el);
      const value = (el.value || el.textContent || '').trim();
      if (value) items.push({ label, value });
    });

    return items;
  }

  // =============== PENGIRIMAN WA DENGAN FALLBACK ===============
  async function sendWhatsApp({
    phone1, phone2, isAbsentMode, statusDisplay,
    nameVal, guestCount, toParam, wishes,
    linkUndangan, eventTitle, filename, extraLines, formNote
  }) {
    const baseText =
      `${eventTitle}\n` +
      `Name: ${nameVal}\n` +
      `Guests: ${guestCount}\n` +
      `To: ${toParam || '-'}\n` +
      `Status: ${statusDisplay}\n` +
      (wishes ? `Wishes: ${wishes}\n` : '') +
      (extraLines ? `${extraLines}` : '') +
      (extraLines && !extraLines.endsWith('\n') ? '\n' : '') +
      `Link: ${linkUndangan}`;

    // helper kirim teks
    async function sendText(body, phone) {
      const fd = new FormData();
      fd.append('action', 'send_whatsapp');
      fd.append('wa_type', 'text');
      fd.append('to', phone);
      fd.append('text_body', body);
      const r = await fetch(AJAX_URL, { method: 'POST', body: fd, credentials: 'same-origin' });
      const txt = await r.text(); let j = {}; try { j = JSON.parse(txt); } catch(_) {}
      return r.ok && j && (j.ok !== false);
    }

    // helper kirim media
    async function sendMedia(dataUrl, caption, outFilename, phone) {
      const fd = new FormData();
      fd.append('action', 'send_whatsapp');
      fd.append('wa_type', 'media');
      fd.append('to', phone);
      fd.append('imageData', dataUrl);
      fd.append('filename', outFilename);
      fd.append('caption', caption);
      const r = await fetch(AJAX_URL, { method: 'POST', body: fd, credentials: 'same-origin' });
      const txt = await r.text(); let j = {}; try { j = JSON.parse(txt); } catch(_) {}
      return r.ok && j && (j.ok !== false);
    }

    // Kirim gambar hanya ke nomor pertama jika status bukan "Tidak Hadir"
    let dataUrl = null;
    if (!isAbsentMode) {
      try {
        dataUrl = await captureAreaAsJPEG(0.85);
      } catch (_) {}
    }

    // Kirim gambar ke nomor pertama jika tersedia
    if (dataUrl) {
      await sendMedia(dataUrl, baseText, filename, phone1);
    } else {
      await sendText(baseText, phone1);
    }

    // Gabungkan pesan default untuk nomor kedua dan lanjutkan pesan lainnya
    if (phone2) {
      const defaultMessage = "Seseorang baru saja mengisi formulir RSVP pada undangan Anda.\n" + baseText;
      await sendText(defaultMessage, phone2);
    }

    // Kirim pesan dari form-field-note ke nomor pertama jika tidak hadir
    if (!isAbsentMode && formNote) {
      await sendText(formNote, phone1);
    }
  }

  // =============== HOOK: SETELAH FORM SUKSES (Elementor) ===============
  jQuery(document).on('submit_success', async function(e) {
    const form = e.target;
    if (!form) return;

    // form target (baru & lama)
    const isTargetForm =
      form.matches('#commentform, form[name="comment_form"], #commentform_id, form[name="comment_form_id"]');
    if (!isTargetForm) return;

    if (form.__waSentOnce) return;
    form.__waSentOnce = true;

    // WA hanya bila ada nomor
    const phoneInput1 = form.querySelector('#form-field-whatsapp');
    const phoneInput2 = form.querySelector('#form-field-whatsapp2');
    const phoneRaw1   = phoneInput1 ? (phoneInput1.value || '').trim() : '';
    const phoneRaw2   = phoneInput2 ? (phoneInput2.value || '').trim() : '';
    const note        = form.querySelector('#form-field-note') ? form.querySelector('#form-field-note').value.trim() : '';

    if (!phoneRaw1 && !phoneRaw2) { form.__waSentOnce = false; return; }

    // Attendance: ambil teks label yg terlihat & deteksi absen
    let statusDisplay = '';
    let isTidakHadir  = false;
    (function () {
      const radios = form.querySelectorAll('input[name="form_fields[attendance]"]');
      const checked = Array.from(radios).find(r => r.checked);
      if (checked) {
        statusDisplay = getRadioLabelText(checked, form);
        isTidakHadir  = isAbsent(checked, statusDisplay);
        return;
      }
      const sel = form.querySelector('#form-field-attendance');
      if (sel && sel.tagName === 'SELECT') {
        const opt = sel.selectedOptions && sel.selectedOptions[0] ? sel.selectedOptions[0] : null;
        statusDisplay = (opt ? (opt.textContent || opt.label || opt.value) : sel.value || '').trim();
        const ds = (opt && opt.dataset && opt.dataset.status) ? opt.dataset.status.toLowerCase() : '';
        if (ds === 'absent') isTidakHadir = true;
        else if (ds === 'attend') isTidakHadir = false;
        else isTidakHadir = ABSENT_PATTERNS.some(rx => rx.test((statusDisplay + ' ' + (opt?.value||'')).toLowerCase()));
        return;
      }
      const any = form.querySelector('#form-field-attendance');
      if (any) {
        statusDisplay = (any.value || any.textContent || '').trim();
        const ds = (any.getAttribute('data-status') || '').toLowerCase();
        if (ds === 'absent') isTidakHadir = true;
        else if (ds === 'attend') isTidakHadir = false;
        else isTidakHadir = ABSENT_PATTERNS.some(rx => rx.test(statusDisplay.toLowerCase()));
      }
    })();

    // Field lain
    const urlParams     = new URLSearchParams(window.location.search);
    const toParam       = urlParams.get('to') || '';
    const nameVal       = (form.querySelector('#form-field-name')?.value || 'Guest').trim();
    const guestCount    = (form.querySelector('#form-field-field_b0d08a4')?.value || '1').trim();
    const wishes        = (form.querySelector('#form-field-comment')?.value || '').trim();
    const linkUndangan  = (form.querySelector('#form-field-link_undangan')?.value || window.location.href).trim();
    const eventTitle    = getEventTitle();

    // Field tambahan dinamis
    const extras = collectExtraFields(form);
    const extraLines = extras.length
      ? extras.map(({label, value}) => `${label}: ${value}`).join('\n') + '\n'
      : '';

    const pathname  = window.location.pathname;
    const slug      = pathname.split('/').filter(Boolean).pop() || 'qr';
    const filename  = `${slug}-to-${(toParam || nameVal).replace(/\s+/g,'_')}.jpg`;

    try {
      await sendWhatsApp({
        phone1: phoneRaw1,
        phone2: phoneRaw2,
        isAbsentMode: isTidakHadir,
        statusDisplay,
        nameVal, guestCount, toParam, wishes, linkUndangan, eventTitle, filename,
        extraLines, formNote: note
      });
    } catch (err) {
      // console.error('WA error:', err);
    } finally {
      setTimeout(() => { form.__waSentOnce = false; }, 500);
    }
  });
});
</script>
